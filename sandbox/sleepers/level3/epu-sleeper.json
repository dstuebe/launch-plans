{
  "services":{
    "sleeper_epu_controller":"res/apps/epu-controller.app",
    "sleeper_epu_sa":"res/apps/epu-sensor-aggregator.app"
  },
  "local_app_confs":{
  },
  "pythoncc":{
    "broker":"${broker_ip_address}",
    "sysname":"${exchange_scope}",
    "broker_heartbeat":"5",
    "log_level":"${ion_log_level}"
  },
  "appretrieve":{
    "retrieve_method":"${epu_retrieve_method}",
