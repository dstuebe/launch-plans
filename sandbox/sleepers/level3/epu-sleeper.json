{
  "services":{
    "sleeper_epu_controller":"res/apps/epu-controller.app"
  },
  "universal_app_confs":{
    "broker_host_list":"${broker_ip_address}",
    "sysname":"${exchange_scope}"
  },
  "local_app_confs":{
    "epu.ionproc.epu_controller":{
        "servicename":"${controller_name}",
        "queue_name_work":"${queue_name_work}",
        "engine_class":"epu.decisionengine.impls.NpreservingEngine",
        "engine_conf":"{\"preserve_n\":\"${preserve_n}\", \"force_site\":\"${worker_iaas_site}\", \"provisioner_vars\":{\"exchange_scope\":\"${exchange_scope}\", \"epuworker_image_id\":\"${worker_id}\", \"broker_ip_address\":\"${broker_ip_address}\", \"retrieve_method\":\"${epu_retrieve_method}\", \"archive_url\":\"${package_repo}/${epu_archive_name}\",        \"package_repo\":\"${package_repo}\", \"git_repo\":\"${epu_git_repo}\", \"git_branch\":\"${epu_git_branch}\", \"git_commit\":\"${epu_git_commit}\", \"ion_log_level\":\"${ion_log_level}\"} }"
    }
  },
  "pythoncc":{
    "broker":"${broker_ip_address}",
    "sysname":"${exchange_scope}",
    "broker_heartbeat":"5",
    "log_level":"${ion_log_level}"
  },
  "appretrieve":{
    "retrieve_method":"${epu_retrieve_method}",
    "archive_url":"${package_repo}/${epu_archive_name}",
    "git_repo":"${epu_git_repo}",
    "git_branch":"${epu_git_branch}",
    "git_commit":"${epu_git_commit}"
  },
  "appinstall":{
    "install_method":"py_venv_setup",
    "package_repo":"${package_repo}"
  },
  "apprun":{
    "run_method":"sh"
  },
  "username":"cc",
  "recipes":["burned_ubuntu64_separated"]
}
