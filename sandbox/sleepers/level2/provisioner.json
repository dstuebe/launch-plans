{
  "services":{
    "dtrs":{
        "service_config":"res/apps/epu-dtrs.app"
    },
    "provisioner":{
        "service_config":"res/apps/epu-provisioner.app"
    },
    "killer":{
        "service_config":"res/apps/epu-killer.app",
        "autostart":false
    }
  },
  "local_app_confs":{
    "epu.ionproc.provisioner":{
        "nimbus_key":"${nimbus_key}",
        "nimbus_secret":"${nimbus_secret}",
        "ec2_key":"${ec2_key}",
        "ec2_secret":"${ec2_secret}",
        "query_period":5
    }
  },
  "universal_app_confs":{
    "broker_host_list":"${broker_ip_address}",
    "sysname":"${exchange_scope}"
  },
  "pythoncc":{
    "broker":"${broker_ip_address}",
    "sysname":"${exchange_scope}",
    "broker_heartbeat":"5",
    "log_level":"${ion_log_level}"
  },
  "appretrieve":{
    "retrieve_method":"${epu_retrieve_method}",
    "archive_url":"${package_repo}/${epu_archive_name}",
    "git_repo":"${epu_git_repo}",
    "git_branch":"${epu_git_branch}",
    "git_commit":"${epu_git_commit}"
  },
  "appinstall":{
    "install_method":"py_venv_setup",
    "package_repo":"${package_repo}"
  },
  "apprun":{
    "run_method":"supervised"
  },
  "username":"cc",
  "groupname":"users",
  "recipes":["r1app"]
}
