  "universal_app_confs":{
    "broker_host_list":"${broker_ip_address}",
    "sysname":"${exchange_scope}"
  },
  "local_app_confs":{
    "ion.test.iontest":{
      "broker_host": "${broker_ip_address}",
      "broker_port": 5672,
      "broker_vhost": "/",
      "broker_heartbeat":0
    },
    "ion.integration.eoi.agent.java_agent_wrapper":{
      "dataset_agent_jar_path":"/home/cc/app/lib/eoi-agents-0.3.1.jar"
    } 
  },
  "virtualenv": {
    "path" : "/home/cc/app-venv",
    "virtualenv" : "/opt/python2.5/bin/virtualenv",
    "python" : "python2.5"
  },
  "pythoncc":{
    "broker":"${broker_ip_address}",
    "sysname":"${exchange_scope}",
    "broker_heartbeat":"5",
    "log_level":"${ion_log_level}"
  },
  "appretrieve":{
    "retrieve_method":"git",
    "archive_url":"notused",
    "git_repo":"https://github.com/ooici/ion-integration.git",
    "git_branch":"r1c3_int2",
    "git_commit":"HEAD"
  },
  "appinstall":{
    "install_method":"javapy_venv_buildout_ant",
    "package_repo":"http://ooici.net/releases",
    "ant_target":"-lib /home/cc/app get-eoi-agents",
    "super_cache":"http://ooici.net/releases/supercache.tar.gz"
  },
  "apprun":{
    "run_method":"sh"
  },
  "username":"cc",
  "groupname":"users",
