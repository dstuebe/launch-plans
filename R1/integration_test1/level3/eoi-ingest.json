{
  "services":{
    "${eoi_ingest_controller_name}":{
        "service_config":"res/apps/epu-controller.app",
        "ION_CONFIGURATION_SECTION":"${eoi_ingest_controller_name}"
    }
  },
  "universal_app_confs":{
    "broker_host_list":"${broker_ip_address}",
    "sysname":"${exchange_scope}"
  },
  "local_app_confs":{
    "${eoi_ingest_controller_name}":{
        "servicename":"${eoi_ingest_controller_name}",
        "queue_name_work":"${queue1_name_work}",
        "engine_class":"epu.decisionengine.impls.NpreservingEngine",
        "engine_conf":"{\"preserve_n\":\"${preserve_n}\", \"force_site\":\"${worker_iaas_site}\", \"epuworker_type\":\"${worker_deployable_type}\", \"provisioner_vars\":{\"exchange_scope\":\"${exchange_scope}\", \"epuworker_image_id\":\"${worker_id}\", \"queue_name_work\":\"${queue1_name_work}\", \"broker_ip_address\":\"${broker_ip_address}\", \"retrieve_method\":\"${integration_retrieve_method}\", \"archive_url\":\"${package_repo}/${integration_archive_name}\",        \"package_repo\":\"${package_repo}\", \"git_repo\":\"${integration_git_repo}\", \"git_branch\":\"${integration_git_branch}\", \"git_commit\":\"${integration_git_commit}\", \"ion_log_level\":\"${ion_log_level}\"} }"
    }
  },
  "pythoncc":{
    "broker":"${broker_ip_address}",
    "sysname":"${exchange_scope}",
    "broker_heartbeat":"5",
    "log_level":"${ion_log_level}"
  },
  "appretrieve":{
    "retrieve_method":"${epu_retrieve_method}",
    "archive_url":"${package_repo}/${epu_archive_name}",
    "git_repo":"${epu_git_repo}",
    "git_branch":"${epu_git_branch}",
    "git_commit":"${epu_git_commit}"
  },
  "appinstall":{
    "install_method":"py_venv_setup",
    "package_repo":"${package_repo}"
  },
  "apprun":{
    "run_method":"sh"
  },
  "username":"cc",
  "groupname":"users",
  "recipes":["user", "r1app"]
}
